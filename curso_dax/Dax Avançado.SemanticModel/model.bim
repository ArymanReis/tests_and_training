            "displayFolder": "Aula 8",
            "expression": [
              "",
              " SWITCH(",
              "    TRUE(),",
              "    ISINSCOPE(dim_subcategoria[Subcategoria]), \"Subcategoria\",",
              "    ISINSCOPE(dim_marca[Marca]), \"Marca\",",
              "    ISINSCOPE(dim_categoria[Categoria]), \"Categoria\",",
              "    BLANK()",
              " )",
              ""
            ],
            "lineageTag": "1e583aaf-9e8f-499b-be1d-1df2387feadc"
          },
          {
            "name": "% Share Proporcional",
            "displayFolder": "Aula 9",
            "expression": [
              "",
              "VAR total_geral = CALCULATE([Receita Bruta], ALLSELECTED(fato_vendas))",
              "VAR total_marca = CALCULATE([Receita Bruta], ALLSELECTED(dim_categoria[Categoria]))",
              "VAR total_categoria = CALCULATE([Receita Bruta], ALLSELECTED(dim_subcategoria[Subcategoria]))",
              "",
              "RETURN",
              "SWITCH(",
              "    TRUE(),",
              "    [Receita Bruta] = BLANK(), BLANK(),",
              "    ISINSCOPE(dim_subcategoria[Subcategoria]), ",
              "        DIVIDE([Receita Bruta], total_categoria, 0),",
              "    ISINSCOPE(dim_categoria[Categoria]), ",
              "        DIVIDE([Receita Bruta], total_marca, 0),",
              "    DIVIDE([Receita Bruta], total_geral, 0)",
              "    )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "lineageTag": "597dd7e6-3f83-47b1-a2d4-b8dcd7ee1767"
          },
          {
            "name": "Rank",
            "displayFolder": "Aula 10",
            "expression": [
              "",
              "VAR categoria =  RANKX(",
              "        ALL(dim_categoria[Categoria]),",
              "        [Medida Dinâmica],,",
              "        DESC",
              "    )",
              " VAR subcategoria =  RANKX(",
              "        ALL(dim_subcategoria[Subcategoria]),",
              "        [Medida Dinâmica],,",
              "        DESC",
              "    )",
              " VAR produto =  RANKX(",
              "        ALL(dim_produto[Descricao_Produto]),",
              "        [Medida Dinâmica],,",
              "        DESC",
              "    )",
              "RETURN",
              "SWITCH(",
              "    TRUE(),",
              "    [Medida Dinâmica] = BLANK(), BLANK(),",
              "    ISINSCOPE(dim_produto[Descricao_Produto]), produto, ",
              "    ISINSCOPE(dim_subcategoria[Subcategoria]), subcategoria, ",
              "    ISINSCOPE(dim_categoria[Categoria]), categoria,",
              "    BLANK()",
              ")",
              ""
            ],
            "formatString": "0",
