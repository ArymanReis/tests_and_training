          {
            "name": "% Pareto Simples",
            "displayFolder": "Aula 15",
            "expression": [
              "",
              "",
              "VAR total = CALCULATE([Receita Bruta], ALLSELECTED(dim_subcategoria[Subcategoria]))",
              "",
              "RETURN",
              "    DIVIDE(",
              "        [Receita Bruta Acumulada Rank],",
              "        total,",
              "        0",
              "    )",
              ""
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "lineageTag": "11fc07f6-bf0d-49aa-8b7d-99aa3197be88"
          },
          {
            "name": "RB Acc Pareto",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"currencyCulture\":\"pt-BR\"}"
              }
            ],
            "displayFolder": "Aula 15",
            "expression": [
              "",
              " VAR categoria = CALCULATE([Receita Bruta],TOPN([Rank],ALLSELECTED(dim_categoria[Categoria]), [Receita Bruta], DESC))",
              " VAR subcategoria = CALCULATE([Receita Bruta],TOPN([Rank],ALLSELECTED(dim_subcategoria[Subcategoria]), [Receita Bruta], DESC))",
              "  VAR produto = CALCULATE([Receita Bruta],TOPN([Rank],ALLSELECTED(dim_produto[Descricao_Produto]), [Receita Bruta], DESC))",
              "",
              "  RETURN",
              "  SWITCH(",
              "    TRUE(),",
              "    ISINSCOPE(dim_produto[Descricao_Produto]), produto,",
              "    ISINSCOPE(dim_subcategoria[Subcategoria]), subcategoria,",
              "    ISINSCOPE(dim_categoria[Categoria]), categoria",
              "  )"
            ],
            "formatString": "\"R$\"\\ #,0;-\"R$\"\\ #,0;\"R$\"\\ #,0",
            "lineageTag": "a08cac72-c577-44d9-9e25-7d700de882fd"
          },
          {
            "name": "% Pareto",
            "displayFolder": "Aula 15",
            "expression": [
              "",
              "",
              "VAR total = CALCULATE([Receita Bruta], ALLSELECTED(fato_vendas))",
              "",
              "RETURN",
              "    DIVIDE(",
              "        [RB Acc Pareto],",
              "        total,",
              "        0",
              "    )",
              ""
            ],
            "formatString": "0%;-0%;0%",
            "lineageTag": "627011a2-f329-4630-b215-2883e1ac9426"
          },
          {
            "name": "Condicional Pareto",
            "displayFolder": "Aula 15",
            "expression": [
              "",
              "IF(",
              "    [% Pareto] <= 0.8, ",
              "    1,",
              "    0",
              ")"
            ],
            "formatString": "0",
            "lineageTag": "0bfeb7b3-927a-4904-9f9c-7a9428ad1193"
          },
          {
            "name": "Total Movél Receita Bruta",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ],
            "displayFolder": "Aula 16",
            "expression": [
              "",
              "VAR data = MAX(dim_calendario[Data])",
              "",
              "RETURN",
              "CALCULATE(",
              "    [Receita Bruta],",
              "    DATESBETWEEN(dim_calendario[Data],",
              "    data - [Valor aux_MeidaMovel],",
              "    data)",
              "    )",
              "    "
            ],
            "lineageTag": "64001df0-32c9-419e-acc0-d9875c1b57ca"
          },
          {
            "name": "Média Movél",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ],
            "displayFolder": "Aula 16",
            "expression": [
              "",
              "VAR data = MAX(dim_calendario[Data])",
              "",
              "RETURN",
              "AVERAGEX(",
              "    DATESBETWEEN(dim_calendario[Data],",
              "    data - [Valor aux_MeidaMovel],",
              "    data",
              "    ),",
              "    [Receita Bruta]",
              "    )",
              "    "
